{
  "name": "apiflighttest",
  "version": "1.0.0",
  "description": "Api for Test",
  "main": "src/app.js",
  "scripts": {
    "start:debug": "env-cmd -e default,debug -r .env.js nodemon src/app.js",
    "start:dev": "env-cmd -e default,development -r .env.js nodemon src/app.js",
    "start:qa": "env-cmd -e default,qa -r .env.js node src/app.js",
    "build:qa": "env-cmd -e default,build,qa -r .env.js npm run _build",
    "build:development": "env-cmd -e default,build,development -r .env.js npm run _build",
    "build:production": "env-cmd -e default,build,production -r .env.js npm run _build",
    "build:start": "node dist/app.js",
    "test": "env-cmd -e default,test -r .env.js jest --verbose --silent",
    "test:watch": "env-cmd -e default,test -r .env.js npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugocruzlfc/apiFlightTest.git"
  },
  "author": "Hugo Cruz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hugocruzlfc/apiFlightTest/issues"
  },
  "homepage": "https://github.com/hugocruzlfc/apiFlightTest#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-validator": "^6.14.1",
    "http": "^0.0.1-security",
    "method-override": "^3.0.0",
    "mongoose": "^6.3.5",
    "mongoose-paginate-v2": "^1.6.3",
    "prettier-stylelint": "^0.4.2",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3"
  }
}
